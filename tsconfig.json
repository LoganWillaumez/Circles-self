{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
		"checkJs": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
  "references": [
    {
      "path": {
        "@circles-self/types": ["./@circles-self/types/*"]
      }
    }
  ]
}
