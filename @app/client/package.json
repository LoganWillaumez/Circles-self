    "@sveltejs/kit": "^1.21.0",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.24",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.4",
    "svelte-check": "^3.4.4",
    "tailwindcss": "^3.3.2",
